[user]
	name = hvenkatraman
	email = hvenkatraman9@gmail.com
[http]
	postBuffer = 524288000
	version = HTTP/1.1
[url "https://"]
	insteadOf = git://
[core]
	editor = vim
	autocrlf = input
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[difftool "vimdiff"]
	cmd = vim -d \"$LOCAL\" \"$REMOTE\"
[alias]
	gloag = git log --oneline --all --graph
